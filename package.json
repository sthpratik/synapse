{
  "name": "synapse-surge",
  "version": "1.0.0",
  "description": "Dynamic load testing tool with intelligent version comparison using K6, pixelmatch, and Sharp",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "synapse": "dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "examples/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docs:serve": "docsify serve docs",
    "docs:init": "docsify init docs",
    "docs:version": "node scripts/update-docs-version.js",
    "mcp:build": "cd mcp-server && npm install && npm run build",
    "mcp:dev": "cd mcp-server && npm run dev",
    "mcp:start": "cd mcp-server && npm start",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run build && npm run docs:version",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "load-testing",
    "k6",
    "performance",
    "cli",
    "yaml",
    "testing",
    "api-testing",
    "stress-testing",
    "benchmark",
    "image-comparison",
    "version-comparison",
    "pixelmatch",
    "sharp",
    "webp",
    "jpeg",
    "png",
    "gif"
  ],
  "author": {
    "name": "Manish Shrestha",
    "email": "sth.pratik@gmail.com",
    "url": "https://github.com/sthpratik"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sthpratik/Synapse.git"
  },
  "bugs": {
    "url": "https://github.com/sthpratik/Synapse/issues"
  },
  "homepage": "https://github.com/sthpratik/Synapse#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@types/jpeg-js": "^0.3.0",
    "@types/sharp": "^0.31.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "csv-parse": "^5.6.0",
    "csv-parser": "^3.0.0",
    "joi": "^18.0.1",
    "jpeg-js": "^0.4.4",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0",
    "sharp": "^0.34.5",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.5",
    "docsify-cli": "^4.4.4",
    "jest": "^30.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "preferGlobal": true
}
